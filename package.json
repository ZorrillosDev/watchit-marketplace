  "importSort": {
    ".ts, .tsx": {
      "style": "module-and-prefix",
      "parser": "typescript"
    },
    ".js, .jsx": {
      "style": "module-and-prefix",
      "parser": "babylon"
    }
  },
  "standard": {
    "ignore": [
      "webpack.config.babel.js",
      "package.json",
      ".gitignore",
      "/coverage/**",
      "/jscpd/**",
      "/webpack/**/*.js",
      "/test/**/*.js"
    ],
    "env": [
      "mocha"
    ]
  },
  "ts-standard": {
    "project": "tsconfig.json",
    "ignore": [
      "webpack.config.babel.js",
      "package.json",
      ".gitignore",
      "/src/i18n",
      "/coverage/**",
      "/jscpd/**",
      "/webpack/**/*.js",
      "/test/**/*.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run hook"
    }
  }
